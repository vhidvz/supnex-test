{
  "name": "supnex-test",
  "version": "0.0.1",
  "description": "Platform with a microservice design",
  "author": "Vahid V. <vhid.vz@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/supnex-test/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/supnex-test/test/jest-e2e.json",
    "command": "node -r tsconfig-paths/register -r ts-node/register libs/command/src/main.ts",
    "command:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register libs/command/src/main.ts",
    "db:clean": "npm run command -- db clean",
    "db:mock": "npm run command -- db mock",
    "db:debug": "npm run command:debug -- db",
    "mongo:drop": "npm run command -- mongo drop",
    "mongo:mock": "npm run command -- mongo mock",
    "mongo:debug": "npm run command:debug -- mongo",
    "doc": "npx compodoc -p tsconfig.json -n \"Supnex Documentation\" -d ./docs -s"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/platform-express": "^9.4.2",
    "@typegoose/typegoose": "^11.2.0",
    "mongoose": "^7.2.2",
    "nest-commander": "^3.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.2.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.4"
  }
}
